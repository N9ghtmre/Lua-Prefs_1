function setvalue(address, flags, value)
  MUCOTOO("Modify address value(Address, value type, value to be modified)")
  local tt = {}
  tt[1] = {}
  tt[1].address = address
  tt[1].flags = flags
  tt[1].value = value
  gg.setValues(tt)
end--Fá´œÉ´á´„á´›Éªá´É´

